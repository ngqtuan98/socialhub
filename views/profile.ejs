<!DOCTYPE html>
<html lang="en">

<head>

  <!-- Meta Tag -->
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">

  <!-- SEO -->
  <meta name="description" content="150 words">
  <meta name="author" content="uipasta">
  <meta name="copyright" content="company name">
  <meta name="robots" content="index,follow">


  <title>Thông tin Sinh viên</title>

  <!-- Favicon -->
  <link rel="shortcut icon" href="images/icons/icon-72x72.png">
  <link rel="apple-touch-icon" sizes="144x144" type="image/x-icon" href="images/icons/icon-144x144.png">

  <!-- All CSS Plugins -->
  <link rel="stylesheet" type="text/css" href="/contentsinhvien/css/plugin.css">
  <link rel="manifest" href="/manifest.json">
  <!-- Main CSS Stylesheet -->
  <link rel="stylesheet" type="text/css" href="/contentsinhvien/css/style.css">

  <!-- Google Web Fonts  -->
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Poppins:400,300,500,600,700">

  <link rel="manifest" href="/manifest.json">
  <!-- HTML5 shiv and Respond.js support IE8 or Older for HTML5 elements and media queries -->
  <!--[if lt IE 9]>
	   <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
	   <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->


</head>

<body>



  <!-- Preloader Start -->
  <div class="preloader">
    <div class="rounder"></div>
  </div>
  <!-- Preloader End -->
  <div id="main">
    <div class="container">
      <div class="row">
        <!-- About Me (Left Sidebar) Start -->
        <div class="col-md-3">
          <div class="about-fixed">

            <div class="my-pic">
              <img src="images/icons/icon-512x512.png" alt="">
              <a href="javascript:void(0)" class="collapsed" data-target="#menu" data-toggle="collapse"><i
                  class="icon-menu menu"></i></a>
              <div id="menu" class="collapse">
                <ul class="menu-link">
                    <li><a href="Trangchu">Trang chủ</a></li>
                  <li><a href="Profile">Profile</a></li>
                  <li><a href="Hoatdong">Hoạt động</a></li>
                  <li><a href="/">Log out</a></li>
                </ul>
              </div>
            </div>

            <div class="my-detail">

              <div class="white-spacing">
                <h1> <%= ten %></h1>
                <span><%= lop %></span>
              </div>
            </div>
          </div>
        </div>
        <!-- About Me (Left Sidebar) End -->
        <!-- Blog Post (Right Sidebar) Start -->
        <div class="col-md-9">
          <div class="col-md-12 page-body">
            <div class="row">
              <div class="sub-title">
                <h2>Thông tin cá nhân</h2>
              </div>
              <div class="col-md-12 content-page">
                <!-- Blog Post Start -->
                <div class="col-md-12 blog-post">

                  <div class="post-info">
                    <span>Họ và tên: <%= ten %></span>
                  </div>
                  <div class="post-info">
                    <span>MSSV: <%= masv %></span>
                  </div>
                  <div class="post-info">
                    <span>Phái: <%= phai %></span>
                  </div>
                  <div class="post-info">
                    <span>Ngày sinh: <%= ngaysinh %></span>
                  </div>
                  <div class="post-info">
                    <span>Nơi sinh: <%= noisinh %></span>
                  </div>
                  <div class="post-info">
                    <span>Ngành: <%= nganh %></span>
                  </div>
                  <div class="post-info">
                    <span>Hệ Đào tạo: <%= hedt %></span>
                  </div>
                  <div class="post-info">
                    <span>Khóa học: <%= khoahoc %></span>
                  </div>
                  <div class="post-info">
                    <span>SDT: <%= sdt %></span>
                  </div>
                  <div class="post-info">
                    <span>Email: <%= email %></span>
                  </div>

                  <% if(vaodoan==1){ %>
                  <div class="post-info">
                    <span style="color: #41b73b;">Vào đoàn: Đã vào đoàn </span>
                  </div>
                  <div class="post-info">
                    <span style="color: #41b73b;">Ngày vào đoàn: <%= ngayvd %></span>
                  </div>
                  <div class="post-info">
                    <span style="color: #41b73b;">Chức vụ: <%= chucvu %></span>
                  </div>
                  <% } %>
                  <% if(vaodoan==0){ %>
                  <div class="post-info">
                    <span style="color:red">Vào đoàn: Chưa vào đoàn </span>
                  </div>
                  <div class="post-info">
                    <span style="color: red;">Ngày vào đoàn: Trống</span>
                  </div>
                  <div class="post-info">
                    <span style="color: red;">Chức vụ: Trống</span>
                  </div>
                  <% } %>
                  <% if(vaodoan==2){ %>
                    <div class="post-info">
                        <span style="color:blueviolet">Chuyển sinh hoạt đoàn: <%= tinhtrang %> </span>
                      </div>
                    <% } %>
                    <% if(vaodoan==3){ %>
                      <div class="post-info">
                        <span style="color:#41b73b">Chuyển sinh hoạt đoàn: <%= tinhtrang %> </span>
                      </div>                     
                      <% } %>
                  <div class="post-info">
                    <span>Khoa: <%= khoa%></span>
                  </div>
                  <% if(vaodoan==0){ %>
                  <a href="chuyensinhhoatdoan" class="button button-style button-anim fa fa-long-arrow-right"><span>Đăng
                      ký sinh hoạt Đoàn tại đây</span></a>
                  <% } %>
                  <% if(vaoHoi==0){ %>
                    <div class="post-info">
                        <span style="color: red;">Bạn chưa vào Hội Sinh viên </span>
                      </div>
                    <a href="thamgiahsv/" class="button button-style button-anim fa fa-long-arrow-right"><span>Đăng
                        ký Hội viên tại đây</span></a>
                    <% } %>
                    <% if(vaoHoi==1){ %>
                      <div class="post-info">
                          <span style="color: #41b73b;">Đã tham gia vào Hội Sinh viên </span>
                        </div>
                      <% } %>
                </div>
                <!-- Blog Post End -->

                <!-- Footer Start -->
                <div class="col-md-12 page-body margin-top-50 footer">
                  <footer>
                    <ul class="menu-link">
                        <li><a href="trangchu">Trang chủ</a></li>
                      <li><a href="profile">Profile</a></li>
                      <li><a href="hoatdong">Hoạt động</a></li>
                      <li><a href="/">Log out</a></li>
                    </ul>
                    <!--<p>© Copyright 2016 DevBlog. All rights reserved</p>-->
                    <!-- UiPasta Credit Start -->
                    <!-- <div class="uipasta-credit">Design By <a href="http://www.uipasta.com" target="_blank">UiPasta</a></div>-->
                    <!-- UiPasta Credit End -->
                  </footer>
                </div>
                <!-- Footer End -->
              </div>
              <!-- Blog Post (Right Sidebar) End -->

            </div>
          </div>
        </div>



        <!-- Back to Top Start -->
        <a href="#" class="scroll-to-top"><i class="fa fa-long-arrow-up"></i></a>
        <!-- Back to Top End -->


        <!-- All Javascript Plugins  -->
        <script type="text/javascript" src="/contentsinhvien/js/jquery.min.js"></script>
        <script type="text/javascript" src="/contentsinhvien/js/plugin.js"></script>
        <!-- Main Javascript File  -->
        <script type="text/javascript" src="/contentsinhvien/js/scripts.js"></script>

        <script src="/Scripts/main.js"></script>
</body>

</html>